{
    "name": "chindit/commitstrip-downloader",
    "description": "A simple command app to download strips from Commitstrip",
    "type": "command",
    "version": "1.1.1",
    "require": {
        "ext-dom": "*",
        "php": "^8.0",
        "symfony/console": "^6.1",
        "symfony/dom-crawler": "^6.1",
        "symfony/flex": "^2.2",
        "symfony/http-client": "^6.1",
        "symfony/css-selector": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^6.1",
        "symfony/phpunit-bridge": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "license": "GNU GPL v3",
    "authors": [
        {
            "name": "David Lumaye",
            "email": "chindit@gmx.us"
        }
    ],
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
