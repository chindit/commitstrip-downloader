{
    "name": "chindit/commitstrip-downloader",
    "description": "A simple command app to download strips from Commitstrip",
    "type": "command",
    "version": "1.0.2",
    "require": {
        "ext-dom": "*",
        "php": "^7.2",
        "symfony/console": "^4.3",
        "symfony/dom-crawler": "^4.3",
        "symfony/flex": "^1.4",
        "symfony/http-client": "^4.3",
        "symfony/css-selector": "^4.3"
    },
    "require-dev": {
        "symfony/test-pack": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "license": "GNU GPL v3",
    "authors": [
        {
            "name": "David Lumaye",
            "email": "chindit@gmx.us"
        }
    ],
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
